{
  "name": "reflux",
  "version": "0.2.7",
  "description": "A simple library for uni-directional dataflow application architecture inspired by ReactJS Flux",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "mocha ./test/**/*.spec.*",
    "docgen": "docco ./src/*.js",
    "lint": "eslint ./src",
    "watch": "onchange './src' './test' -- npm test",
    "benchmark": "node test/benchmarks"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "author": "Mikael Brassman (https://github.com/spoike)",
  "contributors":["Brian Vanderbusch (https://github.com/LongLiveCHIEF) <brian@hackerhappyhour.com>"],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/spoike/reflux.git"
  },
  "keywords": [
    "react",
    "flux"
  ],
  "dependencies": {
    "rx": "^2.5.3"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.9",
    "babelify": "^6.1.2",
    "benchmark": "^1.0.0",
    "browserify": "~10.2.3",
    "chai": "^2.3.0",
    "chai-as-promised": "^5.0.0",
    "docco": "^0.7.0",
    "eslint": "^0.22.1",
    "mocha": "~2.2.5",
    "sinon": "^1.10.3"
  }
}
